    "files": [
      "./test/toArray.js",
      "./test/toBoolean.js",
      "./test/toDate.js",
      "./test/toError.js",
      "./test/toFunction.js",
      "./test/toGenerator.js",
      "./test/toMap.js",
      "./test/toNull.js",
      "./test/toNumber.js",
      "./test/toObject.js",
      "./test/toPromise.js",
      "./test/toRegExp.js",
      "./test/toSet.js",
      "./test/toString.js",
      "./test/toSymbol.js",
      "./test/toUndefined.js",
      "./test/utils/miscellaneous.js",
      "./test/utils/objectClass.js",
      "./test/utils/prune.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "./test/helpers/setup-browser-env.js"
